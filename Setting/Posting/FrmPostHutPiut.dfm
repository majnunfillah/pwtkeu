    Connection = DM.MyStock
    CursorType = ctStatic
    Parameters = <>
    SQL.Strings = (
      'Select a.*, b.Keterangan,c.Keterangan,'
      'Case when A.Tipe='#39'L'#39' then '#39'[L]urus'#39
      '            when A.Tipe='#39'M'#39' then '#39'[M]enurun'#39
      '            when A.Tipe='#39'P'#39' then '#39'[P]ajak'#39
      '            else '#39#39
      '       end Metode'
      'from dbPostHutPiut a, dbPerkiraan b, dbperkiraan c'
      'where a.Perkiraan=b.Perkiraan and c.Perkiraan=a.Akumulasi '
      'Order by a.Kode,a.Perkiraan')
