  object QuDetail: TADOQuery
    AutoCalcFields = False
    CacheSize = 2000
    Connection = DM.MyStock
    CursorType = ctStatic
    CommandTimeout = 0
    DataSource = DsView
    Parameters = <
      item
        Name = 'kodeaktiva'
        DataType = ftString
        NumericScale = 255
        Precision = 255
        Size = 30
        Value = '1600-2000.00001'
      end>
    SQL.Strings = (
      
        'Select a.Perkiraan KodeAktiva,a.Keterangan,Cast(A.Persen as varc' +
        'har(12))+'#39'%'#39' Susut,'
      '       Case when A.Tipe='#39'L'#39' then '#39'[L]urus'#39
      '            when A.Tipe='#39'M'#39' then '#39'[M]enurun'#39
      '            when A.Tipe='#39'P'#39' then '#39'[P]ajak'#39
      '            else '#39#39
      '       end Metode,'
      
        '       b.Bulan,b.Tahun,b.Awal,b.AwalD,b.MD,b.DMD,b.MK,b.DMK,b.Ak' +
        'hir,b.AkhirD,'
      
        '       b.AwalSusut,b.AwalSusutD,b.SD,b.DSD,b.SK,b.DSK,b.AkhirSus' +
        'ut,b.AkhirSusutD,'
      
        '       CAST(B.Tahun as varchar(4))+Case when b.Bulan<10 then '#39'0'#39 +
        ' else '#39#39' end+CAST(B.Bulan as Varchar(2)) Periode,'
      '       B.Akhir-B.AkhirSusut NilaiAK'
      'from DBAKTIVA A'
      
        '     left outer join DBAKTIVADET b on b.Perkiraan=A.Perkiraan an' +
        'd b.Devisi=A.Devisi'
      'Where A.Perkiraan=:kodeaktiva'
      
        'Order by A.Perkiraan,CAST(B.Tahun as varchar(4))+Case when b.Bul' +
        'an<10 then '#39'0'#39' else '#39#39' end+CAST(B.Bulan as Varchar(2))')
    Left = 141
    Top = 160
  end
