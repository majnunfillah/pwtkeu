  object QuView: TADOQuery
    AutoCalcFields = False
    CacheSize = 2000
    Connection = DM.MyStock
    CursorType = ctStatic
    CommandTimeout = 0
    Parameters = <
      item
        Name = '0'
        DataType = ftString
        Size = 2
        Value = '01'
      end
      item
        Name = '1'
        DataType = ftString
        Size = 1
        Value = '0'
      end>
    SQL.Strings = (
      'Declare @Devisi varchar(15),@TipeAktiva tinyint'
      'Select @Devisi=:0,@TipeAktiva=:1'
      'select a.Perkiraan KodeAktiva,a.Tanggal,a.Devisi,c.NamaDevisi,'
      '       Case when a.TipeAktiva=0 then '#39'Aktiva Tetap'#39
      '            when a.TipeAktiva=1 then '#39'Aktiva Yang Dibiayakan'#39
      '            when a.Tipeaktiva=2 then '#39'Aktiva Dalam Penyelesaian'#39
      '            else '#39#39
      
        '       end MyTipe,a.TipeAktiva,a.NoMuka GroupAktiva,b.Keterangan' +
        ' NamaPerkiraan,'
      
        '       a.Keterangan,a.kodeBag,d.Namabag,a.Quantity,a.Persen Susu' +
        't,a.Tipe,'
      '       Case when A.Tipe='#39'L'#39' then '#39'[L]urus'#39
      '            when A.Tipe='#39'M'#39' then '#39'[M]enurun'#39
      '            when A.Tipe='#39'P'#39' then '#39'[P]ajak'#39
      '            else '#39#39
      '       end Metode,'
      
        '       a.akumulasi,a.Biaya,a.PErsenBiaya1,a.Biaya2,a.PersenBiaya' +
        '2,'
      '       a.Biaya3,a.PErsenBiaya3,a.Biaya4,a.persenbiaya4,'
      
        '       e.Keterangan KetAkm,a.nobelakang, a.NoBelakang2,a.Kelompo' +
        'k ,persenpajak'
      'from dbaktiva a'
      '     left Outer join dbperkiraan b on b.perkiraan=a.NoMuka'
      '     left outer join dbdevisi c on c.devisi=a.devisi'
      '     left outer join dbbagian d on d.kodebag=a.kodebag'
      '     left outer join dbperkiraan e on e.perkiraan=a.akumulasi'
      '     where ((a.devisi=@Devisi) Or '
      '    (Case when @Devisi=a.Devisi then @Devisi'
      '          else '#39'Semua'#39
      '     end='#39'Semua'#39')) and'
      '    ((a.tipeaktiva=@TipeAktiva) Or'
      '    (Case when @TipeAktiva=0 then 0'
      '          when @TipeAktiva=1 then 1'
      '          when @TipeAktiva=2 then 2'
      '          else 3'
      '    end=3))'
      'Order by a.NoMuka,a.Perkiraan,a.Devisi'
      '')
    Left = 125
    Top = 128
  end
