  object QuTempHutPiut: TADOQuery
    Connection = DM.MyStock
    CursorType = ctStatic
    LockType = ltBatchOptimistic
    Parameters = <>
    SQL.Strings = (
      
        'declare @IDUser varchar(30), @Perkiraan varchar(30), @TipeDK var' +
        'char(1)'
      ''
      'select @IDUser='#39'SA'#39', @Perkiraan='#39#39', @TipeDK='#39#39
      ''
      
        'select  A.NoFaktur+convert(varchar(8),A.Tanggal,102)+right('#39'0000' +
        '0000'#39'+cast(A.Urut as varchar(8)),8)+A.NoRetur MyKey, A.*'
      'from dbTempHutPiut A'
      'left outer join'
      
        '        (select NoFaktur, KodeCustSupp, min(Tanggal) Tanggal fro' +
        'm dbTempHutPiut'
      
        '        where IDUser=@IDUser and Perkiraan=@Perkiraan and TipeDK' +
        '=@TipeDK'
      '        group by NoFaktur, KodeCustSupp'
      
        '        ) B on B.NoFaktur=A.NoFaktur and B.KodeCustSupp=A.KodeCu' +
        'stSupp'
      
        'where A.IDUser=@IDUser and A.Perkiraan=@Perkiraan and A.TipeDK=@' +
        'TipeDK and isnull(A.StatusUID,'#39#39')<>'#39'D'#39
      'order by B.Tanggal, A.NoFaktur, A.Urut'
      '')
