  object QuTransaksi: TADOQuery
    Connection = DM.MyStock
    CursorType = ctStatic
    Parameters = <
      item
        Name = '0'
        DataType = ftString
        NumericScale = 255
        Precision = 255
        Size = 30
        Value = 'BKK/0110/00001/SZZ'
      end>
    SQL.Strings = (
      'select a.*,b.keterangan as namaPerkiraan,d.Keterangan NamaLawan,'
      'case when a.valas<>'#39'IDR'#39' then a.debet+ a.kredit'
      '     else 0'
      'end as Jumlah,'
      'a.DebetRp+a.KreditRp as JumlahRp,c.NamaDevisi,'
      'Case when a.TPHC='#39'C'#39' then '#39'[C]ash'#39
      '     When a.TPHC='#39'T'#39' then '#39'[T]ransfer'#39
      '     when a.TPHC='#39'H'#39' then '#39'[H]utang Giro'#39
      '     when a.TPHC='#39'P'#39' then '#39'[P]iutang Giro'#39
      '     else '#39#39
      'end MyTPHC,e.NamaBag,f.nourut'
      'from dbtransaksi a'
      '     left outer join dbperkiraan b on a.perkiraan=b.perkiraan'
      '     left outer join dbdevisi c on c.Devisi=a.Devisi'
      '     left outer join dbPerkiraan d on d.Perkiraan=a.Lawan'
      '     left outer join dbBagian e on e.kodebag=a.kodebag'
      '     left outer join dbTrans f on f.nobukti=a.nobukti'
      'where a.nobukti=:0 '
      'Order by a.Nobukti,a.Urut'
      '')
