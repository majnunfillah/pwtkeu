    Parameters = <>
    SQL.Strings = (
      'Declare @KodeBrgJd varchar(30)'
      ''
      'select  @kodeBrgjd='#39#39
      ''
      'select A.* ,B.NamaBrg, B.Sat1 from DbBOm A'
      'Left Outer Join DBbarang B on A.KodeBrgBHN = B.Kodebrg'
      'where KodeBrgJd=@KodeBrgJd order by kodebrgBhn'
